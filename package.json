{
  "name": "app",
  "private": true,
  "scripts": {
    "build": "remix vite:build",
    "dev": "shopify app dev",
    "config:link": "shopify app config link",
    "coverage": "vitest run --coverage",
    "generate": "shopify app generate",
    "deploy": "shopify app deploy",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "start": "remix-serve ./build/server/index.js",
    "docker-start": "npm run setup && npm run start",
    "setup": "prisma generate && prisma migrate deploy",
    "setup:local": "node scripts/setup-local.js && yarn prisma migrate dev --schema ./prisma_local/schema.prisma --name jason-remix",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:prettier": "prettier --check \"**/*.{ts,tsx,md}\"",
    "lint:tsc": "tsc --noEmit",
    "shopify": "shopify",
    "prisma": "prisma",
    "prisma:studio:local": "prisma studio --schema ./prisma_local/schema.prisma",
    "graphql-codegen": "graphql-codegen",
    "schema:gx:download": "graphql-codegen --config app/api/shopify/download.ts",
    "schema:gx:typegen": "graphql-codegen --config app/api/shopify/typegen.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "vite": "vite"
  },
  "type": "module",
  "engines": {
    "node": "^18.20 || ^20.10 || >=21.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.2.1",
    "@remix-run/dev": "^2.16.1",
    "@remix-run/fs-routes": "^2.16.1",
    "@remix-run/node": "^2.16.1",
    "@remix-run/react": "^2.16.1",
    "@remix-run/serve": "^2.16.1",
    "@sentry/remix": "^10.10.0",
    "@shopify/app-bridge-react": "^4.1.6",
    "@shopify/polaris": "^12.0.0",
    "@shopify/shopify-app-remix": "^3.7.0",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.0",
    "@tanstack/react-query": "^4.36.1",
    "@vitest/coverage-v8": "^1.5.0",
    "base64url": "^3.0.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "isbot": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.2.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^1.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-query": "^6.0.0",
    "@remix-run/eslint-config": "^2.16.1",
    "@remix-run/route-config": "^2.16.1",
    "@shopify/api-codegen-preset": "^1.1.1",
    "@types/eslint": "^9.6.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.2.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.0.1",
    "graphql": "^16.11.0",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2",
    "vite": "^6.2.2"
  },
  "workspaces": {
    "packages": [
      "extensions/*"
    ]
  },
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ],
  "resolutions": {
    "@graphql-tools/url-loader": "8.0.16",
